{
  "name": "piwo-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/server/index.js",
    "test": "node ./node_modules/mocha/bin/_mocha",
    "migrate": "knex migrate:rollback && knex migrate:latest --env development",
    "seed:save": "node ./src/server/db/saveSeeds.js",
    "seed": "knex seed:run --env development",
    "db:up": "npm run migrate && npm run seed && npm start"
  },
  "keywords": [],
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.17.1"
  },
  "dependencies": {
    "jsonfile": "^4.0.0",
    "knex": "^0.14.4",
    "koa": "^2.5.0",
    "koa-body": "^2.5.0",
    "koa-cors": "0.0.16",
    "koa-router": "^7.4.0",
    "pg": "^7.4.1"
  }
}
